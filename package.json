{
  "name": "seeds-game",
  "version": "1.0.0",
  "description": "A connect the dots game with seeds",
  "scripts": {
    "deploy-production": "npm run build && firebase deploy -P production --token $FIREBASE_TOKEN",
    "deploy-staging:ci": "npm run build && firebase deploy -P staging --token $FIREBASE_TOKEN",
    "deploy-staging": "npm run build && firebase deploy -P staging",
    "dev": "npm run css-build && parcel src/index.html",
    "build": "npm run css-build && parcel build src/index.html",
    "css-build": "postcss src/css/main.css --output assets/style.min.css",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewMacmurray/seeds-game.git"
  },
  "author": "Andrew MacMurray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewMacmurray/seeds-game/issues"
  },
  "homepage": "https://github.com/andrewMacmurray/seeds-game#readme",
  "devDependencies": {
    "@types/howler": "^2.2.2",
    "autoprefixer": "^9.8.6",
    "bounce.js": "^0.8.2",
    "elm": "^0.19.1-5",
    "elm-hot": "^1.1.6",
    "elm-live": "^4.0.2",
    "firebase-tools": "^9.11.0",
    "howler": "^2.2.1",
    "node-elm-compiler": "^5.0.6",
    "parcel-bundler": "^1.12.5",
    "pepjs": "^0.5.3",
    "postcss-clean": "^1.2.2",
    "postcss-cli": "^7.1.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.2.0",
    "postcss-import": "^12.0.1",
    "tachyons-custom": "^4.9.8",
    "typescript": "^3.9.9"
  }
}
