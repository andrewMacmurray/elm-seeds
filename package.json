{
  "name": "elmSeed",
  "version": "1.0.0",
  "description": "A connect the dots game with seeds",
  "scripts": {
    "watch": "npm run live & npm run css",
    "live": "elm-live app/elm/Main.elm --output=public/main.js --dir=public --open --debug --pushstate",
    "css": "postcss --watch --config ./postcss-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewMacmurray/elmSeed.git"
  },
  "author": "Andrew MacMurray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewMacmurray/elmSeed/issues"
  },
  "homepage": "https://github.com/andrewMacmurray/elmSeed#readme",
  "dependencies": {
    "elm": "^0.18.0",
    "tachyons-custom": "^4.5.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "elm-live": "^2.7.4",
    "postcss-clean": "^1.0.2",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0"
  }
}
