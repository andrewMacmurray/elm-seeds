{
  "name": "seeds-game",
  "version": "1.0.0",
  "description": "A connect the dots game with seeds",
  "scripts": {
    "deploy-production": "npm run build && firebase deploy -P production",
    "deploy-staging": "npm run build && firebase deploy -P staging",
    "dev": "npm run css-build && elm-typescript-interop && parcel src/index.html",
    "build": "npm run css-build && elm-typescript-interop && parcel build src/index.html",
    "css-build": "postcss src/css/main.css --output assets/style.min.css",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewMacmurray/seeds-game.git"
  },
  "author": "Andrew MacMurray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewMacmurray/seeds-game/issues"
  },
  "homepage": "https://github.com/andrewMacmurray/seeds-game#readme",
  "devDependencies": {
    "@types/howler": "^2.1.2",
    "autoprefixer": "^9.7.4",
    "bounce.js": "^0.8.2",
    "elm": "^0.19.1-3",
    "elm-hot": "^1.1.4",
    "elm-live": "^4.0.2",
    "elm-typescript-interop": "0.0.17",
    "howler": "^2.1.3",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.4",
    "pepjs": "^0.5.2",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "tachyons-custom": "^4.9.8",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "gsap": "^3.2.6"
  }
}
